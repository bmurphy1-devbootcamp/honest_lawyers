<!-- Sign In -->

<!-- Render signin form partial -->
<h2>Sign in</h2>
<%= form_tag({action: :create}, method: :post) do %>
  <p>
    <%= label_tag(:email, "Email address:") %>
    <%= text_field_tag(:email) %>
  </p>
  <p>
    <%= label_tag(:password, "Password:") %>
    <%= password_field_tag(:password) %>
  </p>
  <%= submit_tag("Log in") %>
<% end %>
